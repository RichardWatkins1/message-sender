{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "outDir": "dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [
      "es2015"
    ],
    "rootDir": "./",
  },
  "exclude": [
    "node_modules",
    "**/__tests__",
    "coverage",
    "jest.config.js",
    "dist"
  ],
}